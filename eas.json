{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "20.9.0",
      "ios": {
        "image": "auto",
        "resourceClass": "m-medium"
      },
      "android": {
        "image": "auto"
      },
      "channel": "base"
    },
    "development": {
      "environment": "development",
      "extends": "base",
      "channel": "development",
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "buildConfiguration": "Debug"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "production": {
      "extends": "base",
      "channel": "production",
      "environment": "production",
      "ios": {
        "simulator": true,
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      }
    }
  },
  "submit": {
    "production": {}
  }
}